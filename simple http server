//example http sever 
const express = require('express')
const app = express()
const port = 3000

app.get('/name', function(req, res)  {
  res.json({
    name: "benin",
    age:22
  })
})

app.get('/', function(req, res)  {
    res.send('Hello World!')
  })

app.listen(port, function() {
  console.log(`Example app listening on port ${port}`)
})
